Algoritmo "Teste_Saída"

Var

   a : inteiro
   b : real
   c : caractere
   d : logico

Inicio

      a <- 32
      b <- 2.34567
      c <- "Bom dia!"
      d <- VERDADEIRO

      escreval (a)
      escreval
      escreval (b)
      escreval (b:10)
      escreval (b:10:2)
      escreval
      escreval (c)
      escreval
      escreva (d)
      
Fimalgoritmo