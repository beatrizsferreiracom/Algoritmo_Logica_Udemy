Algoritmo "Teste_Saída"

Var

   idade : inteiro
   salario : real
   nome : caractere
   
Inicio

      idade <- 32
      salario <- 4560.9
      nome <- "Maria Silva"
      
      escreva("A funcionária ", nome, " ganha ", salario:8:2, " e tem ", idade, " anos.")
      
Fimalgoritmo